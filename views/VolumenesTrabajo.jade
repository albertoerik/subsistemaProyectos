extends layoutResidencia
block content
    .row
        nav.navbar.navbar-inverse
            .container-fluid
                .collapse.navbar-collapse(id="bs-example-navbar-collapse-1")
                    ul.nav.navbar-nav.col-md-12
                        li(class="col-md-1")
                            a(href="Trabajador")
                                span(class="glyphicon glyphicon-home", aria-hidden="true")
                        li(class="col-md-2")
                            a(href='Planilla',id='btnNParteD') VER INFORMES
                        li(class="dropdown col-md-2")
                            a(href="#") SALIR
                        li(class="dropdown col-md-2 col-md-offset-3 avatarrr")
    .row(style="margin-top:40px")
        .col-md-12
            .btn-group
                button.btn.btn-success.dropdown-toggle(type='button',id='btnmes' data-toggle='dropdown', aria-haspopup='true', aria-expanded='false',style="margin:0 auto;display:block") Enero
                    span.caret(style="margin-left:5px")
                ul.dropdown-menu.meses
                    li
                        a(href='#') enero
                    li
                        a(href='#') febrero
                    li
                        a(href='#') marzo
                    li
                        a(href='#') abril
                    li
                        a(href='#') mayo
                    li
                        a(href='#') junio
                    li
                        a(href='#') julio
                    li
                        a(href='#') agosto
                    li
                        a(href='#') septiembre
                    li
                        a(href='#') octubre
                    li
                        a(href='#') noviembre
                    li
                        a(href='#') diciembre
    .row.col-md-12
            h5.col-md-2(style="color: #010101") RESIDENCIA:
            h5.col-md-2(style="color: #929292") Acasio
    .row.col-md-12
            h2.col-md-12(style="color: #02219C") VOLUMENES DE TRABAJO 
    table.table.table-bordered.table-condemsed.table-striped(id='tablavolumen')
        thead
            tr.headT(style="background:#CDDEEB; font-size: 11px")
                .row.col-md-12
                    th(id="filaprincipal")(style="background:#CDDEEB; font-size: 11px") FECHA ACTIVIDAD
        tbody#volumenestable

    .row(style='margin:20px;')
        .col-md-12
            btn.btn.btn-primary.btn-lg.col-md-4.col-md-offset-4(type='button',id='btnguardarvolumen') GUARDAR VOLUMEN
    script(src="/javascripts/bootstrap.min.js")